    <case id="repetitionMaxInstanceCountTestTwo" name="Repetition Max Instance Count Test Two" flowable:initiatorVariableName="initiator" flowable:candidateStarterGroups="flowableUser">
        <casePlanModel id="onecaseplanmodel1" name="Case plan model" flowable:formFieldValidation="false">
            <extensionElements>
                <flowable:work-form-field-validation><![CDATA[false]]></flowable:work-form-field-validation>
                <design:stencilid><![CDATA[CasePlanModel]]></design:stencilid>
            </extensionElements>
            <planItem id="planItem1" name="Task A" definitionRef="humanTask1">
                <itemControl>
                    <repetitionRule flowable:counterVariable="repetitionCounter" flowable:maxInstanceCount="1"></repetitionRule>
                    <manualActivationRule></manualActivationRule>
