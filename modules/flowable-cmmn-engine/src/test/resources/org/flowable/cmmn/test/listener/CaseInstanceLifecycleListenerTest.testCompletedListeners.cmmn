<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/CMMN/20151109/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:flowable="http://flowable.org/cmmn" xmlns:cmmndi="http://www.omg.org/spec/CMMN/20151109/CMMNDI" xmlns:dc="http://www.omg.org/spec/CMMN/20151109/DC" xmlns:di="http://www.omg.org/spec/CMMN/20151109/DI" targetNamespace="http://www.flowable.org/casedef">
    <case id="testCaseLifecycleListeners" name="testCaseLifecycleListeners" flowable:initiatorVariableName="initiator">
        <extensionElements>
            <!-- active -> complete -->
            <flowable:caseLifecycleListener sourceState="active" targetState="completed" class="org.flowable.cmmn.test.listener.TestCaseInstanceLifecycleListener"></flowable:caseLifecycleListener>
            <flowable:caseLifecycleListener sourceState="active" targetState="completed" expression="${caseInstance.setVariable('expressionVar', 'planItemIsCompleted')}"></flowable:caseLifecycleListener>
            <flowable:caseLifecycleListener sourceState="" targetState="completed" delegateExpression="${delegateListener}"></flowable:caseLifecycleListener>
        </extensionElements>
        <casePlanModel id="casePlanModel">
            <planItem id="planItem1" name="class delegate" definitionRef="humanTask"></planItem>
            <humanTask id="humanTask" name="class delegate">
            </humanTask>
        </casePlanModel>
    </case>
</definitions>