<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/CMMN/20151109/MODEL" 
    xmlns:dc="http://www.omg.org/spec/CMMN/20151109/DC" 
    xmlns:di="http://www.omg.org/spec/CMMN/20151109/DI"
    xmlns:cmmndi="http://www.omg.org/spec/CMMN/20151109/CMMNDI" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:flowable="http://flowable.org/cmmn"
    targetNamespace="http://flowable.org/cmmn">

    <case id="myCase">
        <casePlanModel id="myPlanModel" name="My CasePlanModel">

            <planItem id="planItem1" name="Task One" definitionRef="serviceTask1" />
            <planItem id="planItem2" name="Task Two" definitionRef="serviceTask2" />

            <task id="serviceTask1" flowable:type="java" flowable:expression="${bean.invoke()}" />
            <task id="serviceTask2" flowable:type="java" flowable:expression="${bean.invoke()}" />

        </casePlanModel>
    </case>

</definitions>
