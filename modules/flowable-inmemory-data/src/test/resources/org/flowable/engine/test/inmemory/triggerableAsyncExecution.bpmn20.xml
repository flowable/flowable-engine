<?xml version="1.0" encoding="UTF-8"?>
<definitions 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:flowable="http://flowable.org/bpmn"
  targetNamespace="Examples">
  <process id="triggerableAsyncExecution" name="Triggerable Execution">
    <startEvent id="theStart" />
    <sequenceFlow sourceRef="theStart" targetRef="service1" />
    <serviceTask id="service1" flowable:class="org.flowable.engine.data.inmemory.TriggerableServiceTask" flowable:triggerable="true" flowable:async="true"/>
    <sequenceFlow sourceRef="service1" targetRef="theEnd" />
    <endEvent id="theEnd" />
  </process>
</definitions>
