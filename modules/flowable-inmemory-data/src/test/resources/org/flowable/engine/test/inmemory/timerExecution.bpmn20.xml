<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:flowable="http://flowable.org/bpmn"
						 xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC"
						 xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema"
						 expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.flowable.org/processdef">
		<process id="timerExecution">
				<startEvent id="theStart" />
				<sequenceFlow id="flow1" sourceRef="theStart" targetRef="firstTask" />
				<userTask id="firstTask" name="First task" />
				<boundaryEvent id="boundaryTimerEvent" attachedToRef="firstTask">
						<timerEventDefinition>
								<timeDuration>PT1H</timeDuration>
						</timerEventDefinition>
				</boundaryEvent>
				<sequenceFlow sourceRef="boundaryTimerEvent" targetRef="throwException" />
				<scriptTask id="throwException" name="Execute script" scriptFormat="juel">
						<script>
This is not JUEL syntax and will throw
						</script>
				</scriptTask>
				<sequenceFlow id="flow3" sourceRef="firstTask" targetRef="theEnd" />
				<endEvent id="theEnd" />
		</process>
</definitions>
